-@post.each do |post|
  =link_to post_path(post.id) do
    .index
      .index-image
        =image_tag post.image.url, class: 'vocabimage', size: "170x170" 
      .index-contents
        =post.vocab
        =post.definition
        =post.example
        =post.created_at.strftime("%Y年%m月%d日 %H時%M分")
